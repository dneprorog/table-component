  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": "last 2 versions"
          }
        }
      ],
      "@babel/react"
    ]
  },
  "dependencies": {
    "gh-pages": "^2.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0",
